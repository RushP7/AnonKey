{
  "manifest_version": 3,
  "name": "AnonKey",
  "version": "0.1",
  "permissions": ["storage", "tabs"],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "action": { "default_popup": "popup.html" },
  "description": "AnonKey",
  "icons": {
    "16": "images/logo.png",
    "32": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "author": "Rushil Punchhi",
  "content_scripts": [
    {
      "js": ["scripts/jquery-3.6.1.min.js", "scripts/content.js"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ]
}
